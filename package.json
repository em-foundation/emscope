{
    "name": "@em-foundation/emscope",
    "version": "25.1.1",
    "description": "benchmark energy efficiency in resource-constrained embedded systems",
    "bin": {
        "emscope": "dist/emscope"
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "esbuild ./src/Main --bundle --external:vscode --external:joulescope_driver --external:jls-writer --external:@serialport/bindings-cpp --outfile=dist/emscope --platform=node --format=cjs",
        "watch": "npm run build -- --sourcemap --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/em-foundation/emscope.git"
    },
    "keywords": [
        "openem",
        "em-foundation",
        "emscope",
        "em",
        "emfoundation",
        "embedded"
    ],
    "author": "The EM Foundation",
    "license": "MIT",
    "preferGlobal": true,
    "bugs": {
        "url": "https://github.com/em-foundation/emscope/issues"
    },
    "homepage": "https://github.com/em-foundation/emscope/blob/main/docs/ReadMore.md",
    "dependencies": {
        "@serialport/bindings-cpp": "^13.0.1",
        "@types/adm-zip": "^0.5.7",
        "@types/js-yaml": "^4.0.9",
        "adm-zip": "^0.5.16",
        "commander": "^14.0.0",
        "jls-writer": "https://github.com/em-foundation/npm-packages/releases/download/resources/jls-writer-0.0.2.tgz",
        "joulescope_driver": "^1.10.0",
        "js-yaml": "^4.1.0",
        "picomatch": "^4.0.3",
        "serialport": "^13.0.0",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@types/picomatch": "^4.0.2",
        "esbuild": "^0.25.8"
    }
}